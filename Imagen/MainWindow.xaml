<Window x:Class="Imagen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="El Gran Opacador" Height="450" Width="800" Icon="img/128.ico">
    <Grid>
        <Image 
            x:Name="foto"
            Source="/img/elGranMasturbador.jpg" 

            Opacity="1" Margin="0,0,227,0"/>
        <Button 
            x:Name="nivel1Boton" 
            Margin="632,70,40,0" 
            Height="23" 
            VerticalAlignment="Top" 
            Click="nivel1Boton_Click"
            Content="Nivel 1" 
            FontSize="15"/>
        <Button 
            x:Name="nivel2Boton" 
            Margin="632,120,40,0" 
            Height="23" 
            VerticalAlignment="Top" 
            Click="nivel2Boton_Click"
            Content="Nivel 2" 
            FontSize="15"/>
        <Button 
            x:Name="nivel3Boton" 
            Margin="632,170,40,0" 
            Height="23" 
            VerticalAlignment="Top" 
            Click="nivel3Boton_Click"
            Content="Nivel 3" 
            FontSize="15"/>
    </Grid>
</Window>
